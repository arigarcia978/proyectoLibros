<% if user_signed_in? %>
   <h1>Listing Users</h1>
   <table>
      <thead>
         <tr>
            <th colspan="3"></th>
         </tr>
      </thead>

      <tbody>
         <% @users.each do |user| %>
            <tr>
               <td><%= user.id %></td>
               <td><%= user.email %></td>
               <td><%= link_to 'Show', user %></td>
               <td><%= link_to 'Edit', edit_user_registration_path %></td>
               <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
         <% end %>
      </tbody>
   </table>
<% end %>

<br>

<td><%= link_to 'Estantes', estantes_path %></td>